{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "jsx": "react",
    "esModuleInterop": true,
    "baseUrl": ".",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true
  },
  "include": ["src", "typings", ".storybook"]
}
