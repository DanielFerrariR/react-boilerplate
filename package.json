{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "setup": "npm install && npm run validate",
    "validate": "npm-run-all --parallel check-types check-format lint:eslint lint:stylelint test",
    "start": "serve --no-clipboard --single --listen 3000 build",
    "dev": "cross-env-shell WEBPACK_DEV_SERVER=true \"webpack-dev-server --mode development\"",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "preanalyze": "cross-env-shell IS_ANALYZE=true npm run build",
    "analyze": "webpack-bundle-analyzer build/bundle-stats.json",
    "check_circular": "cross-env-shell CHECK_CIRCULAR_DEPENDENCIES=true npm run build:dev",
    "check_circular:write": "cross-env-shell CHECK_CIRCULAR_DEPENDENCIES=write npm run build:dev",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx,.md,.mdx",
    "lint:stylelint": "npx stylelint **/*.scss",
    "format": "prettier --write .",
    "check-format": "prettier --list-different .",
    "check-types": "tsc",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --watch"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "warpjs": "^1.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@storybook/addon-a11y": "^7.6.17",
    "@storybook/addon-actions": "^7.6.17",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/preset-ant-design": "^0.0.2",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^7.6.17",
    "@storybook/react-webpack5": "^7.6.17",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/dom": "^8.18.1",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^16.18.98",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/svgo": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "buffer": "^6.0.3",
    "circular-dependency-plugin": "^5.2.2",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "eslint": "8.28.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-webpack": "0.13.8",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.6",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-mdx": "2.0.5",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-testing-library": "^5.10.3",
    "eslint-plugin-tsdoc": "0.2.17",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "is-ci-cli": "2.1.2",
    "jest": "^29.7.0",
    "jest-axe": "9.0.0",
    "jest-raw-loader": "1.0.1",
    "jest-transform-stub": "2.0.0",
    "lint-staged": "^12.3.7",
    "mini-css-extract-plugin": "^2.9.0",
    "npm-run-all": "^4",
    "postcss-loader": "^7.3.4",
    "prettier": "^3.3.1",
    "remark-preset-lint-consistent": "5.1.1",
    "remark-preset-lint-markdown-style-guide": "5.1.2",
    "remark-preset-lint-recommended": "6.1.2",
    "remark-preset-prettier": "2.0.1",
    "sass": "^1.69.7",
    "sass-loader": "^13.3.3",
    "serve": "14.2.3",
    "storybook": "^7.6.17",
    "stream-browserify": "^3.0.0",
    "stylelint": "^14.6.1",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-scss": "^4.2.0",
    "svgo": "3.2.0",
    "typescript": "^5.4.5",
    "util": "^0.12.5",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^5.10.0",
    "clean-webpack-plugin": "^4.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "dotenv-webpack": "2.0.0"
  },
  "resolutions": {
    "wrap-ansi": "7.0.0",
    "string-width": "4.1.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,mdx}": "eslint --cache --fix",
    "*.scss": "stylelint --cache --fix",
    "*.{js,jsx,ts,tsx,json,md,mdx,html,scss}": "prettier --write"
  }
}
